{
    "browser": {
        "headless": false,
        "maximize": true,
        "proxy": {
            "enabled": false,
            "host": "127.0.0.1",
            "port": 8081
        },
        "profile": {}
    },
    "crawler": {
        "entryPoint": "http://localhost:9000/file-upload",
        "eventTimeout": 10000,
        "maxDuration": 600000,
        "elements": [
            "a",
            "button",
            "input[type=\"submit\"]"
        ],
        "maxDepth": 10,
        "authentication": {
            "pptrRecording": "/home/astra/Downloads/pptr.json"
        },
        "includeRegexes": [
            "https?://localhost:9000(?:/.*|)"
        ]
    }
}